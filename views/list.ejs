<%-include("header")-%>
    <div class="box" id="heading">
    <h1 style="color: white;"><%=listofItem%></h1>
    </div>
  
    <div class="box">
       <% for(var i=0;i<newListItems.length;i++){ %>
           <div class="item">
            <input type="checkbox" name="" id="">
            <p><%=newListItems[i]%></p>
        </div>
            <% } %>
       <form  class =" item" action="/" method="post">
        <input type="text" name="newItem" id="" placeholder="Add New Item">
        <button type="submit" class="button" name="button" value="<%=listofItem%>">+</button>
    </form>

    </div>
   
<%-include("footer")-%>


   
 
